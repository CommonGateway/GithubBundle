{
  "$id": "https://opencatalogi.nl/repository.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "version": "0.1",
  "title": "Repository",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "example": "1296269",
      "required": true
    },
    "node_id": {
      "type": "string",
      "example": "MDEwOlJlcG9zaXRvcnkxMjk2MjY5",
      "required": true
    },
    "name": {
      "type": "string",
      "example": "Hello-World",
      "description": "The name of the repository.",
      "required":true
    },
    "full_name": {
      "type": "string",
      "example": "octocat/Hello-World",
      "required": true
    },
    "owner": {
      "$ref": "https://opencatalogi.nl/simpleUser.schema.json",
      "required": true
    },
    "private": {
      "type": "boolean",
      "description": "Whether the repository is private or public.",
      "default": true,
      "required": true
    },
    "html_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "fork": {
      "type": "boolean",
      "required": true
    },
    "url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "archive_url": {
      "type": "string",
      "required": true
    },
    "assignees_url": {
      "type": "string",
      "required": true
    },
    "blobs_url": {
      "type": "string",
      "required": true
    },
    "branches_url": {
      "type": "string",
      "required": true
    },
    "collaborators_url": {
      "type": "string",
      "required": true
    },
    "comments_url": {
      "type": "string",
      "required": true
    },
    "commits_url": {
      "type": "string",
      "required": true
    },
    "compare_url": {
      "type": "string",
      "required": true
    },
    "contents_url": {
      "type": "string",
      "required": true
    },
    "contributors_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "deployments_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "downloads_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "events_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "forks_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "git_commits_url": {
      "type": "string",
      "required": true
    },
    "git_refs_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "git_tags_url": {
      "type": "string",
      "required": true
    },
    "git_url": {
      "type": "string",
      "required": true
    },
    "issue_comment_url": {
      "type": "string",
      "required": true
    },
    "issue_events_url": {
      "type": "string",
      "required": true
    },
    "issues_url": {
      "type": "string",
      "required": true
    },
    "keys_url": {
      "type": "string",
      "required": true
    },
    "labels_url": {
      "type": "string",
      "required": true
    },
    "languages_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "merges_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "milestones_url": {
      "type": "string",
      "required": true
    },
    "notifications_url": {
      "type": "string",
      "required": true
    },
    "pulls_url": {
      "type": "string",
      "required": true
    },
    "releases_url": {
      "type": "string",
      "required": true
    },
    "ssh_url": {
      "type": "string",
      "required": true
    },
    "stargazers_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "statuses_url": {
      "type": "string",
      "required": true
    },
    "subscribers_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "subscription_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "tags_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "teams_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "trees_url": {
      "type": "string",
      "required": true
    },
    "clone_url": {
      "type": "string",
      "required": true
    },
    "mirror_url": {
      "type": "string",
      "format": "uri",
      "nullable": true
    },
    "hooks_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "svn_url": {
      "type": "string",
      "format": "url",
      "required": true
    },
    "homepage": {
      "type": "string",
      "format": "url",
      "nullable": true
    },
    "organization": {
      "$ref": "https://opencatalogi.nl/simpleUser.schema.json",
      "nullable": true
    },
    "language": {
      "type": "string",
      "nullable": true
    },
    "forks": {
      "type": "integer",
      "required": true
    },
    "forks_count": {
      "type": "integer",
      "required": true
    },
    "stargazers_count": {
      "type": "integer",
      "required": true
    },
    "watchers_count": {
      "type": "integer",
      "required": true
    },
    "size": {
      "type": "integer",
      "description": "The size of the repository. Size is calculated hourly. When a repository is initially created, the size is 0.",
      "required": true
    },
    "default_branch": {
      "type": "string",
      "description": "The default branch of the repository.",
      "required": true
    },
    "open_issues": {
      "type": "integer",
      "required": true
    },
    "open_issues_count": {
      "type": "integer",
      "required": true
    },
    "watchers": {
      "type": "integer",
      "required": true
    },
    "is_template": {
      "type": "boolean",
      "description": "Whether this repository acts as a template that can be used to generate new repositories.",
      "default": false
    },
    "license": {
      "$ref": "https://opencatalogi.nl/licenseSimple.schema.json",
      "required": true
    },
    "topics": {
      "type": "array"
    },
    "has_issues": {
      "type": "boolean",
      "description": "Whether issues are enabled.",
      "default": true,
      "required": true
    },
    "has_projects": {
      "type": "boolean",
      "description": "Whether projects are enabled.",
      "default": true,
      "required": true
    },
    "has_wiki": {
      "type": "boolean",
      "description": "Whether the wiki is enabled.",
      "default": true,
      "required": true
    },
    "has_pages": {
      "type": "boolean",
      "required": true
    },
    "has_downloads": {
      "type": "boolean",
      "description": "Whether downloads are enabled.",
      "default": true,
      "required": true
    },
    "has_discussions": {
      "type": "boolean",
      "description": "Whether discussions are enabled.",
      "default": false,
      "required": true
    },
    "archived": {
      "type": "boolean",
      "description": "Whether the repository is archived.",
      "default": false,
      "required": true
    },
    "disabled": {
      "type": "boolean",
      "description": "Returns whether or not this repository disabled.",
      "required": true
    },
    "visibility": {
      "type": "string",
      "description": "The repository visibility: public, private, or internal.",
      "default": "public",
      "enum": [
        "public",
        "private",
        "internal"
      ]
    },
    "pushed_at": {
      "type": "string",
      "format": "date-time",
      "nullable": true
    },
    "created_at": {
      "type": "string",
      "format": "date-time",
      "required": true
    },
    "updated_at": {
      "type": "string",
      "format": "date-time",
      "nullable": true
    },
    "permissions": {
      "$ref": "https://opencatalogi.nl/permissions.schema.json"
    },
    "allow_rebase_merge": {
      "type": "boolean",
      "description": "Whether to allow rebase merges for pull requests.",
      "default": true
    },
    "temp_clone_token": {
      "type": "string"
    },
    "allow_squash_merge": {
      "type": "boolean",
      "description": "Whether to allow squash merges for pull requests.",
      "default": true
    },
    "allow_auto_merge": {
      "type": "boolean",
      "description": "Whether to allow Auto-merge to be used on pull requests.",
      "default": false
    },
    "delete_branch_on_merge": {
      "type": "boolean",
      "description": "Whether to delete head branches when pull requests are merged.",
      "default": false
    },
    "allow_merge_commit": {
      "type": "boolean",
      "description": "Whether to allow merge commits for pull requests.",
      "default": true
    },
    "subscribers_count": {
      "type": "integer",
      "required": true
    },
    "network_count": {
      "type": "integer",
      "required": true
    }
  },
  "required": [
    "name"
  ]
}