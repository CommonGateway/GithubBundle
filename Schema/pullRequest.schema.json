{
    "$id": "https://opencatalogi.nl/pullRequest.schema.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "version": "0.1.0",
    "title": "Pull Request",
    "type": "object",
    "properties": {
        "merged_at": {
            "type": "string",
            "format": "date-time",
            "nullable":true
        },
        "diff_url": {
            "type": "string",
            "format": "uri",
            "nullable":true,
            "required":true
        },
        "html_url": {
            "type":"string",
            "format": "uri",
            "nullable":true,
            "required":true
        },
        "patch_url": {
            "type": "string",
            "format": "uri",
            "nullable":true,
            "required":true
        },
        "url": {
            "type": "string",
            "format": "uri",
            "nullable":true,
            "required":true
        }
    }
}