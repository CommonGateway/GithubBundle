{
"$id": "https://opencatalogi.nl/repository.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "version": "0.1",
  "title": "Repository",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "example": "1296269"
    },
    "node_id": {
      "type": "string",
      "example": "MDEwOlJlcG9zaXRvcnkxMjk2MjY5"
    },
    "name": {
      "type": "string",
      "example": "Hello-World",
      "required": true,
      "description": "The name of the repository."
    },
    "full_name": {
      "type": "string",
      "example": "octocat/Hello-World"
    },
    "owner": {
      "$ref": "https://opencatalogi.nl/ownerRepository.schema.json"
    },
    "private": {
      "type": "boolean",
      "description": "Whether the repository is private.",
      "default": true
    },
    "html_url": {
      "type": "string",
      "format": "url",
      "example": "https://github.com/octocat/Hello-World"
    },
    "description": {
      "type": "string",
      "description": "A short description of the repository."
    },
    "fork": {
      "type": "boolean"
    },
    "url": {
      "type": "string",
      "format": "url"
    },
    "archive_url": {
      "type": "string",
      "format": "url"
    },
    "assignees_url": {
      "type": "string",
      "format": "url"
    },
    "blobs_url": {
      "type": "string",
      "format": "url"
    },
    "branches_url": {
      "type": "string",
      "format": "url"
    },
    "collaborators_url": {
      "type": "string",
      "format": "url"
    },
    "comments_url": {
      "type": "string",
      "format": "url"
    },
    "commits_url": {
      "type": "string",
      "format": "url"
    },
    "compare_url": {
      "type": "string",
      "format": "url"
    },
    "contents_url": {
      "type": "string",
      "format": "url"
    },
    "contributors_url": {
      "type": "string",
      "format": "url"
    },
    "deployments_url": {
      "type": "string",
      "format": "url"
    },
    "downloads_url": {
      "type": "string",
      "format": "url"
    },
    "events_url": {
      "type": "string",
      "format": "url"
    },
    "forks_url":{
      "type": "string",
      "format": "url"
    },
    "git_commits_url": {
      "type": "string",
      "format": "url"
    },
    "fit_refs_url": {
      "type": "string",
      "format": "url"
    },
    "git_tags_url": {
      "type": "string",
      "format": "url"
    },
    "git_url": {
      "type": "string",
      "format": "url"
    },
    "issue_comment_url": {
      "type": "string",
      "format": "url"
    },
    "issue_events_url": {
      "type": "string",
      "format": "url"
    },
    "issues_url": {
      "type": "string",
      "format": "url"
    },
    "keys_url": {
      "type": "string",
      "format": "url"
    },
    "labels_url": {
      "type": "string",
      "format": "url"
    },
    "languages_url": {
      "type": "string",
      "format": "url"
    },
    "merges_url": {
      "type": "string",
      "format": "url"
    },
    "milestones_url": {
      "type": "string",
      "format": "url"
    },
    "notifications_url": {
      "type": "string",
      "format": "url"
    },
    "pulls_url": {
      "type": "string",
      "format": "url"
    },
    "releases_url": {
      "type": "string",
      "format": "url"
    },
    "ssh_url": {
      "type": "string",
      "format": "url"
    },
    "stargazers_url": {
      "type": "string",
      "format": "url"
    },
    "statuses_url": {
      "type": "string",
      "format": "url"
    },
    "subscribers_url": {
      "type": "string",
      "format": "url"
    },
    "subscription_url": {
      "type": "string",
      "format": "url"
    },
    "tags_url": {
      "type": "string",
      "format": "url"
    },
    "teams_url": {
      "type": "string",
      "format": "url"
    },
    "trees_url": {
      "type": "string",
      "format": "url"
    },
    "clone_url": {
      "type": "string",
      "format": "url"
    },
    "mirror_url": {
      "type": "string",
      "format": "url"
    },
    "hooks_url": {
      "type": "string",
      "format": "url"
    },
    "svn_url": {
      "type": "string",
      "format": "url"
    },
    "homepage": {
      "type": "string",
      "format": "url",
      "description": "A URL with more information about the repository."
    },
    "language": {
      "nullable": true
    },
    "forks_count": {
      "type": "integer"
    },
    "stargazers_count": {
      "type": "integer"
    },
    "watchers_count": {
      "type": "integer"
    },
    "size": {
      "type": "integer"
    },
    "default_branch": {
      "type": "string",
      "example": "master"
    },
    "open_issues_count": {
      "type": "integer"
    },
    "is_template": {
      "type": "boolean",
      "default": false
    },
    "topics":{
      "type": "array"
    },
    "has_issues": {
      "type": "boolean",
      "default": "true"
    },
    "has_projects": {
      "type": "boolean",
      "default": true
    },
    "has_wiki": {
      "type": "boolean",
      "default": true
    },
    "has_pages": {
      "type": "boolean"
    },
    "has_downloads": {
      "type": "boolean",
      "default": true

    },
    "has_discussions": {
      "type": "boolean"
    },
    "archived": {
      "type": "boolean"
    },
    "disabled": {
      "type": "boolean"
    },
    "visibility": {
      "type": "string",
      "anyOf":[
        "public",
        "private",
        "internal"
      ]
    },
    "pushed_at": {
      "type": "string",
      "format":"date"
    },
    "created_at": {
      "type": "string",
      "format":"date"
    },
    "updated_at": {
      "type": "string",
      "format":"date"
    },
    "permissions": {

    }
  },
  "required": [
    "name"
  ]
}